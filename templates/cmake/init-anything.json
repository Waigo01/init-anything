{
  "name": "Cmake",
  "runCommands": [
    {
      "name": "start",
      "commands": [{"command": "cmake ..", "execDir": "./build"}, {"command": "make", "execDir": "./build"}, {"command": "./$projectName", "execDir": "./build"}]
    }
  ],
  "varFiles": ["./CMakeLists.txt"],
  "vars": [{"name": "projectName", "reqFor": "init, start"}]
}
